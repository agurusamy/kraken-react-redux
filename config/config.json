{
  "express": {
    "view engine": "jsx",
    "view": "require:react-engine/lib/expressView",
    "view cache": false,
    "views": "path:./public/views"
  },

  "view engines": {
      "jsx": {
          "module": "react-engine/lib/server",
          "renderer": {
            "method": "create",
              "arguments": [{
                  "routes": "require:./public/routes",
                  "routesFilePath": "path:./public/routes",
                  "reduxStoreInitiator": "path:./public/store"
              }]
          }
      }
  },

  "middleware": {
    "static": {
      "enabled": true,
      "priority": 40,
      "name": "serve-static",
      "module": {
          "arguments": ["path:dist/"]
      }
    },
    "router": {
      "module": {
        "arguments": [{"directory": "path:./routes"}]
      }
    }
  }
}
